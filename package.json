      "jsonwebtoken": "^8.4.0",
      "method-override": "^3.0.0",
      "mongoose": "^5.4.11",
      "multer": "^1.4.1",
      "passport": "^0.4.0",
      "passport-jwt": "^4.0.0",
      "validator": "^10.11.0"
   },
   "devDependencies": {
